{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true,
        "objectLiteralShorthandMethods": true
    },
    "env": {
        "browser": true,
        "node": true,
    },
    "rules": {
        "quotes": [2, "single"],
        "key-spacing": false,
        "no-multi-spaces": false,
        "no-underscore-dangle": false
    }
}
